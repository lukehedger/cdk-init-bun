{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "types": ["bun-types"]
  },
  "exclude": ["**/test", "cdk.out", "node_modules"],
  "extends": "@tsconfig/node18/tsconfig.json",
  "include": ["stacks"]
}
